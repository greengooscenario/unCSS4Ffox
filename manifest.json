{
  "manifest_version": 2,
  "name": "unCSS",
  "version": "0.1",
  "description": "Deactivate CSS for the current page. Reload page to reactivate.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.svg",
    "default_title": "CSS off"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "browserSettings"
  ]
}

